<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.projetoressoflix">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ressoflixicon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ressoflixicon_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Projetoressoflix">
        <activity android:name=".homeresso"></activity>
        <activity android:name=".MainActivity" android:screenOrientation="sensor">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>